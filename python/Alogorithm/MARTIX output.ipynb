    "a = np.empty((n,n))\n",
    "seed = s\n",
    "for  j in range(0,n) :\n",
    "    for i in range(0,j+1) :\n",
    "        a[i,j] = seed  \n",
    "        seed = seed+1\n",
    "        if seed == 10:\n",
    "            seed = 1\n",
    "    \n",
    "print(a)"
